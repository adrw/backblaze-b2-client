{
  "name": "backblaze-b2-client",
  "description": "Rich client for Backblaze B2",
  "version": "0.0.11",
  "license": "Apache-2.0",
  "author": {
    "name": "Andrew Alexander (adrw)"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrw/backblaze-b2-client.git"
  },
  "main": "index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=4.0.0 <=4.1.1"
  },
  "dependencies": {
    "@gideo-llc/backblaze-b2-upload-any": "0.1.3",
    "async": "3.2.0",
    "backblaze-b2": "1.5.0",
    "connect-lastmile": "1.2.2",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "fs-extra": "^8.0.0",
    "lodash": "4.17.15",
    "lodash.chunk": "4.2.0",
    "underscore": "1.9.2"
  },
  "devDependencies": {
    "@misk/prettier": "0.1.26-56",
    "@types/fs-extra": "8.1.0",
    "@types/jest": "25.1.4",
    "@types/node": "11.9.5",
    "dayjs": "1.8.7",
    "jest": "25.2.3",
    "tslib": "1.11.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "prettier": "@misk/prettier",
  "scripts": {
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "build": "tsc",
    "test": "npm build && jest",
    "lint": "prettier --write --config package.json \".{/src/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "postmerge": "/bin/true",
    "precommit": "npm run-script lint",
    "prepush": "npm test"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1",
      "^test(.*)$": "<rootDir>/test/$1"
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
