{
  "name": "backblaze-b2-client",
  "description": "Rich client for Backblaze B2",
  "version": "0.0.27",
  "license": "Apache-2.0",
  "author": {
    "name": "Andrew Alexander (adrw)"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrw/backblaze-b2-client.git"
  },
  "main": "index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=4.0.0 <=4.1.1"
  },
  "dependencies": {
    "@gideo-llc/backblaze-b2-upload-any": "0.1.3",
    "axios": "0.20.0",
    "axios-retry": "3.1.8",
    "async": "3.2.0",
    "backblaze-b2": "1.5.0",
    "crypto": "1.0.1",
    "connect-lastmile": "2.0.0",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "fs-extra": "^9.0.0",
    "lodash": "4.17.20",
    "lodash.chunk": "4.2.0",
    "underscore": "1.10.1"
  },
  "devDependencies": {
    "@misk/prettier": "0.1.26",
    "@types/fs-extra": "9.0.1",
    "@types/jest": "26.0.10",
    "@types/node": "11.9.5",
    "dayjs": "1.8.34",
    "jest": "26.4.2",
    "tslib": "2.0.1",
    "ts-jest": "26.3.0",
    "typescript": "4.0.2"
  },
  "prettier": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "scripts": {
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "build": "tsc",
    "test": "npm build && jest",
    "lint": "prettier --write --config package.json \".{/src/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "postmerge": "/bin/true",
    "precommit": "npm run-script lint",
    "prepush": "npm test"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1",
      "^test(.*)$": "<rootDir>/test/$1"
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
